<?xml version="1.0" encoding="utf-8"?>
<resources>
  <!--
    model: content-box
    XXX is border-box better?
   -->
  <declare-styleable name="progressbar">
    <attr name="background" format="color" />
    <attr name="foreground" format="color" />
    <attr name="innerRadius" format="dimension" />
    <attr name="outerRadius" format="dimension" />
    <attr name="borderColor" format="color" />
    <attr name="borderWidth" format="dimension" />
    <attr name="degreeFrom" format="float" />
    <attr name="degreeTo" format="float" />
    <attr name="valueFrom" format="float" />
    <attr name="valueTo" format="float" />
    <attr name="value" format="float" />
  </declare-styleable>
</resources>
