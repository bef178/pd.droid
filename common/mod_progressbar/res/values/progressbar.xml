<?xml version="1.0" encoding="utf-8"?>
<resources>
  <!--
    model: content-box
    XXX is border-box better?
   -->
  <declare-styleable name="progressbar">
    <attr name="bgColor" format="color" />
    <attr name="fgColor" format="color" />
    <attr name="innerRadius" format="dimension" />
    <attr name="outerRadius" format="dimension" />
    <attr name="borderColor" format="color" />
    <attr name="borderWidth" format="dimension" />
    <attr name="startDegree" format="float" />
    <attr name="sweptDegree" format="float" />
    <attr name="valueTo" format="float" />
    <attr name="value" format="float" />
  </declare-styleable>
</resources>
