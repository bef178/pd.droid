<?xml version="1.0" encoding="UTF-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
  <item android:state_pressed="true">
    <layer-list>
      <item android:drawable="@drawable/win98_button_state_pressed"/>
      <item android:drawable="@drawable/win98_button_mask_focused"/>
    </layer-list>
  </item>
  <item android:state_focused="true">
    <layer-list>
      <item android:drawable="@drawable/win98_button_state_normal"/>
      <item android:drawable="@drawable/win98_button_mask_focused"/>
    </layer-list>
  </item>
  <item android:drawable="@drawable/win98_button_state_normal"/>
</selector>
